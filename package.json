{
    "name": "game_collection",
    "version": "1.0.0",
    "scripts": {
        "client": "cd client && npm run start",
        "server": "nodemon --inspect server.js",
        "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
        "start": "node ./bin/www",
        "debug": "nodemon --inspect ./server.js",
        "build": "cd client && npm run build",
        "install-deps": "npm install && cd client && npm install"
    },
    "dependencies": {
        "axios": "^0.19.2",
        "dotenv": "^8.2.0",
        "express": "latest",
        "moment": "^2.24.0",
        "mongoose": "^5.9.6",
        "multer": "^1.4.2"
    },
    "devDependencies": {
        "concurrently": "^5.1.0",
        "nodemon": "^2.0.2"
    },
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git://github.com/Minidotz/game_collection.git"
    }
}
